package com.aricionur.model;

public class Person {

	private int id;
    private String name;
    private int surname;

    public Person() {
    }

    public Person(int id, String name, int age) {
        this.id = id;
        this.name = name;
        this.surname = age;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return surname;
    }

    public void setAge(int age) {
        this.surname = age;
    }

}
